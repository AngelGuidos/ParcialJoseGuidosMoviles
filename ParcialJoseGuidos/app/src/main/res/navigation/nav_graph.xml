<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/billboardFragment">

    <fragment
        android:id="@+id/billboardFragment"
        android:name="com.galacticstudio.parcialjoseguidos.ui.lake.billboard.BillboardFragment"
        android:label="fragment_billboard"
        tools:layout="@layout/fragment_billboard" >
        <action
            android:id="@+id/action_billboardFragment_to_lagoFragment"
            app:destination="@id/lagoFragment" />
        <action
            android:id="@+id/action_billboardFragment_to_addLagoFragment"
            app:destination="@id/addLagoFragment" />
    </fragment>
    <fragment
        android:id="@+id/lagoFragment"
        android:name="com.galacticstudio.parcialjoseguidos.ui.lake.LagoFragment"
        android:label="fragment_lago"
        tools:layout="@layout/fragment_lago" />
    <fragment
        android:id="@+id/addLagoFragment"
        android:name="com.galacticstudio.parcialjoseguidos.ui.lake.addlake.AddLagoFragment"
        android:label="fragment_add_lago"
        tools:layout="@layout/fragment_add_lago" />
</navigation>